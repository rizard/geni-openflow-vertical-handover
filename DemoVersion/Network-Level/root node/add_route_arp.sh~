AP2_BR_ETH_IP=192.168.4.201
AP2_BR_ETH_IP_MAC=22:dc:13:80:f2:48

AP1_ETH_IP=130.127.38.2
AP1_ETH_NAT_MAC=00:1d:a2:18:ad:c5 

AP2_ETH_IP=130.127.38.134
AP2_ETH_NAT_MAC=00:1d:a2:18:ad:c5

CLIETN_MAC=12:51:16:90:8f:ee
CLIENT_IP=192.168.4.8
sudo arp -s $CLIENT_IP $CLIENT_MAC

route add $AP1_ETH_IP/32 dev eth5
sudo arp -s $AP1_ETH_IP $AP1_ETH_NAT_MAC

route add $AP2_ETH_IP/32 dev eth6
sudo arp -s $AP2_ETH_IP $AP2_ETH_NAT_MAC

sudo arp -s $AP1_BR_ETH_IP $AP1_BR_ETH_IP_MAC
sudo arp -s $AP2_BR_ETH_IP $AP2_BR_ETH_IP_MAC
